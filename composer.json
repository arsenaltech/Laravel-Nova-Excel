{
  "name": "arsenaltech/laravel-nova-excel",
  "description": "Supercharged Excel exports for Laravel Nova Resources",
  "license": "MIT",
  "keywords": [
    "laravel",
    "phpspreadsheet",
    "phpexcel",
    "nova",
    "laravel-nova",
    "actions"
  ],
  "authors": [
    {
      "name": "Patrick Brouwers",
      "email": "patrick@maatwebsite.nl"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "laravel/nova": "*",
    "arsenaltech/excel": "*"
  },
  "autoload": {
    "psr-4": {
      "Maatwebsite\\LaravelNovaExcel\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Maatwebsite\\LaravelNovaExcel\\LaravelNovaExcelServiceProvider"
      ]
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/arsenaltech/Laravel-Excel.git"
    }
  ]
}
